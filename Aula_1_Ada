        "\n",
        "#Entrada de notas de Lara\n",
        "\n",
        "Lara= int(input(\"Insira a nota da p1(Lara):\"))\n",
        "Larap2= int(input(\"Insira a nota da p2(Lara):\"))\n",
        "Larap3= int(input(\"Insira a nota da p3(Lara):\"))\n",
        "media= (Lara + Larap2 +Larap3)/3\n",
        "Verificação1= media >= 7\n",
        "print(f\"A média das notas é:  {media:.2f} e maior ou igual a 7? {Verificação1}\")\n",
        "\n",
        "#Entrada de notas de Erika\n",
        "\n",
        "Erika= int(input(\"Insira a nota da p1(Erika):\"))\n",
        "Erikap2= int(input(\"Insira a nota da p2 (Erika):\"))\n",
        "Erikap3= int(input(\"Insira a nota da p3(Erika):\"))\n",
        "media1= (Erika + Erikap2 + Erikap3)/3\n",
        "Verificação2= media1 >= 7\n",
        "print(f\"A média das notas é:   {media1:.2f} e maior ou igual a 7? {Verificação2}\")\n",
